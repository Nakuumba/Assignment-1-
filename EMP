begin
    numeric nRoll
    display "ENTER EMPLOYEE CODE : "
    accept nRoll
    switch(nRoll)
    begin
        case 1 : display "BLAKE"
            break;
        case 2 : display "AIDAN"
            break;
        case 3 : display "SHAUN"
            break;
        case 4 : display "ERASTUS"
            break;
        case 5 : display "ASHANTE"
            break;
        case 6 : display "QUINN"
    endcase
end
 
